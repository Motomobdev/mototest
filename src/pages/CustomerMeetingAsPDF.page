        </apex:pageBlockSectionItem>
        <apex:pageBlockSectionItem >
            <apex:outputLabel value="{!$ObjectType.Customer_Meeting__c.fields.Business__c.label}" />
            <apex:outputText value="{!Customer_Meeting__c.Business__c}" />
        </apex:pageBlockSectionItem>
        <apex:pageBlockSectionItem >
            <apex:outputLabel value="{!$ObjectType.Customer_Meeting__c.fields.Location__c.label}" />
            <apex:outputText value="{!Customer_Meeting__c.Location__c}" />
        </apex:pageBlockSectionItem>
        <apex:pageBlockSectionItem >
            <apex:outputLabel value="" />
            <apex:outputText value="" />
        </apex:pageBlockSectionItem>
        <apex:pageBlockSectionItem >
            <apex:outputLabel value="Record Type" />
            <apex:outputText value="{!Customer_Meeting__c.RecordType}" />
        </apex:pageBlockSectionItem>
    
    </apex:pageBlockSection>

</apex:pageBlock>

