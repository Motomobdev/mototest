<apex:page id="page" standardController="Customer_Meeting__c" showHeader="false" sidebar="false" cache="false" >

<img alt="Customer Meeting" title="Customer Meeting" src="/img/icon/custom51_100/measuringTape32.png" style="position: absolute; top: 18px; left: 5px;" />

<apex:sectionHeader title="Customer Meeting" subtitle="{!Customer_Meeting__c.Name}"/>

<apex:pageBlock id="pb1" title="Customer Meeting Detail">

    <apex:pageBlockSection id="pbs1" columns="2" collapsible="false">
    
        <apex:pageBlockSectionItem >
            <apex:outputLabel value="{!$ObjectType.Customer_Meeting__c.fields.Name.label}" />
            <apex:outputText value="{!Customer_Meeting__c.Name}" />
        </apex:pageBlockSectionItem>
        <apex:pageBlockSectionItem >
            <apex:outputLabel value="{!$ObjectType.Customer_Meeting__c.fields.Account__c.label}" />
            <apex:outputText value="{!Customer_Meeting__c.Account__c}" />
        </apex:pageBlockSectionItem>
        <apex:pageBlockSectionItem >
            <apex:outputLabel value="{!$ObjectType.Customer_Meeting__c.fields.Meeting_Start_Date__c.label}" />
            <apex:outputText value="{!Customer_Meeting__c.Meeting_Start_Date__c}" />
        </apex:pageBlockSectionItem>
        <apex:pageBlockSectionItem >
            <apex:outputLabel value="{!$ObjectType.Customer_Meeting__c.fields.Meeting_End_Date__c.label}" />
            <apex:outputText value="{!Customer_Meeting__c.Meeting_End_Date__c}" />
        </apex:pageBlockSectionItem>
        <apex:pageBlockSectionItem >
            <apex:outputLabel value="{!$ObjectType.Customer_Meeting__c.fields.Playbook__c.label}" />
