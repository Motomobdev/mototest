                                    </apex:outputField>
                                    <apex:outputText rendered="{!isExcelRender || inApprovalProccess|| isBundleParent}" value="{0,number,###,##0}">
                                        <apex:param value="{!l.porLineItem.Units__c}"/>
                                    </apex:outputText>
                                </td>
                                <td class="currency-cell showHideOTH" >
