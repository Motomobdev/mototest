        <fullName>Download_Account_Plan</fullName>
        <availability>online</availability>
        <description>Downloads Account Plan in PDF format</description>
        <displayType>button</displayType>
        <encodingKey>UTF-8</encodingKey>
        <height>600</height>
        <linkType>url</linkType>
        <masterLabel>Download Account Plan</masterLabel>
        <openType>sidebar</openType>
        <protected>false</protected>
        <url>/apex/loop__looplus?eid={!Account_Plan__c.Id}&amp;accountId={!Account_Plan__c.AccountId__c}&amp;autorun=true&amp;ddpIds=a2340000000LEGU&amp;sessionid={!$Api.Session_ID}</url>
    </webLinks>
    <webLinks>
        <fullName>Download_Account_Plan_ListView</fullName>
        <availability>online</availability>
        <description>ListView button</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Download Account Plan</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/27.0/connection.js&quot;)}

var planIds = {!GETRECORDIDS($ObjectType.Account_Plan__c)};

if(planIds.length &lt; 1)
{
    alert(&quot;You have not selected any Account Plan. Please select one Account Plan from below list.&quot;);
}
else if(planIds.length &gt; 1)
{
    alert(&quot;You have selected more than one Account Plan. Please select one Account Plan at a time.&quot;);
}
else
{
    var query = &quot;SELECT Id, Name, Account__c FROM Account_Plan__c WHERE Id = &apos;&quot; + planIds[0] + &quot;&apos;&quot;;
    
    var result = sforce.connection.query(query);
    
    var records = result.getArray(&quot;records&quot;);
    
    var url = &quot;/apex/loop__looplus&quot;;
    url += &quot;?eid=&quot; + records[0].Id;
    url += &quot;&amp;accountId=&quot; + records[0].Account__c;
    url += &quot;&amp;sessionid=&quot; + sforce.connection.sessionId;
    url += &quot;&amp;autorun=true&quot;;
    url += &quot;&amp;ddpIds=a2340000000LEGU&quot;;
    url += &quot;&amp;header=false&quot;;
    url += &quot;&amp;sidebar=false&quot;;
    
    var winName = &quot;Download Account Plan - &quot; + records[0].Name;
    
    window.open(url, winName, &quot;location=no,menubar=no,toolbar=no,width=500,height=300&quot;);
}</url>
    </webLinks>
