IF( MOD(MONTH(TODAY()),3) = 0 , FLOOR( MONTH(TODAY()) / 3 ) , FLOOR( MONTH(TODAY()) / 3 ) + 1) = 
IF( MOD(MONTH(End_Date__c),3) = 0 , FLOOR( MONTH(End_Date__c) / 3 ) , FLOOR( MONTH(End_Date__c) / 3 ) + 1) , 

