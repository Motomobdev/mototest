    <fields>
        <fullName>QC_sw_royalties__c</fullName>
        <externalId>false</externalId>
        <formula>IF(QC_Modem__c,
IF(ISPICKVAL(QC_modem_type__c, &quot;MSM7xxx&quot;), &quot;Android phone ... likely uses QCOM MMV &amp; Fluence&quot; &amp; BR(), &quot;&quot;) &amp;
IF(ISPICKVAL(QC_modem_type__c, &quot;QSC60x5&quot;), &quot;Android phone, but uses QCOM as modem-only, likely only using QCOM Fluence&quot; &amp; BR(), &quot;&quot;) &amp;
IF(ISPICKVAL(QC_modem_type__c, &quot;MDM6x00+OMAP&quot;), &quot;Android phone, but uses QCOM as modem-only, likely only using QCOM Fluence&quot; &amp; BR(), &quot;&quot;)&amp;
IF(ISPICKVAL(QC_modem_type__c, &quot;MDM6x00+AP20&quot;), &quot;Android phone, but uses QCOM as modem-only, likely only using QCOM Fluence&quot; &amp; BR(), &quot;&quot;)&amp;
IF(ISPICKVAL(QC_modem_type__c, &quot;MSM6xxx (no OMAP nor AP20)&quot;), &quot;not an Android phone ... possible QCOM SW use:&quot; &amp; BR() &amp;
    &quot;*CMX = audio codecs ... likely for all phones in this category&quot; &amp; BR() &amp;
    &quot;* MMV = video codecs ... likely for all phones in this category&quot; &amp; BR() &amp;
    &quot;* QBT = bluetooth codecs ... very limited use going forward&quot; &amp; BR() &amp;
    &quot;* Fluence = dual-mic noise cancellation ... use is steady, possibly increasing&quot; &amp; BR() &amp;
    &quot;* QVM = QCOM Java ... used only on some Sprint phones (very unlikely)&quot; &amp; BR(), &quot;&quot;) &amp;
IF(ISPICKVAL(QC_modem_type__c, &quot;QSC6xxx (no OMAP nor AP20)&quot;), &quot;not an Android phone ... possible QCOM SW use:&quot; &amp; BR() &amp;
    &quot;*CMX = audio codecs ... likely for all phones in this category&quot; &amp; BR() &amp;
    &quot;* MMV = video codecs ... likely for all phones in this category&quot; &amp; BR() &amp;
    &quot;* QBT = bluetooth codecs ... very limited use going forward&quot; &amp; BR() &amp;
    &quot;* Fluence = dual-mic noise cancellation ... use is steady, possibly increasing&quot; &amp; BR() &amp;
    &quot;* QVM = QCOM Java ... used only on some Sprint phones (very unlikely)&quot; &amp; BR(), &quot;&quot;), &quot;Not a QC modem, no QC royalties&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>QC sw royalties</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
