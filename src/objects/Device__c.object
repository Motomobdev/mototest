    <validationRules>
        <fullName>Android_Platform_Rule</fullName>
        <active>false</active>
        <description>If Android is chosen as the platform then the user must select an Android Release.</description>
        <errorConditionFormula>AND(
     ISPICKVAL(Platform__c, &quot;Android&quot;),
      ISPICKVAL(Android_Release__c, &quot;&quot;))</errorConditionFormula>
        <errorMessage>Please select the &quot;Android Release&quot; located on the Device object.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Android_Platform_Version_Rule</fullName>
        <active>false</active>
        <description>Platform Version is required if Android Platform is selected.</description>
        <errorConditionFormula>AND( 
ISPICKVAL(Platform__c, &quot;Android&quot;),
ISPICKVAL(MAPS_Versions__c, &quot;&quot;),
NOT( $Profile.Name = &quot;MotoDev - SysAdmin&quot;))</errorConditionFormula>
        <errorMessage>Please complete the Platform Version field location on the Device object.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Build_Fingerprint_Last_Submitted</fullName>
        <active>true</active>
        <description>Enter in the new Fingerprint being submitted</description>
        <errorConditionFormula>AND
(ISBLANK(GMS_Last_SW_Submitted__c),

ISPICKVAL( GMS_Submission_Status__c , &quot;Checklist Provided&quot;))</errorConditionFormula>
        <errorDisplayField>GMS_Last_SW_Submitted__c</errorDisplayField>
        <errorMessage>Enter the Build Fingerprint being submitted for approval</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Device_Fully_Approved_Rule</fullName>
        <active>false</active>
        <description>When Device Fully Approved equals Yes, lock the record except for MotoReady Admin</description>
        <errorConditionFormula>AND( ISPICKVAL(PRIORVALUE(Device_Fully_Approved__c), &quot;Yes&quot;), 
AND(
NOT( $Profile.Name = &quot;MotoDev - SysAdmin&quot;), 
NOT($Profile.Name = &quot;MotoDev - SW BOM&quot;),
NOT($Profile.Name = &quot;MotoDev - MotoReady Admin&quot;), 
NOT($Profile.Name = &quot;MotoDev - Google TA Process Admin&quot;),
NOT($Profile.Name = &quot;MotoDev - Google TA Process&quot;),
NOT($Profile.Name = &quot;MotoDev - Partnership Management&quot;),
NOT($Profile.Name = &quot;MotoDev - Partnership Management (non-Paul)&quot;),
NOT($Profile.Name = &quot;MotoDev - Partnership Management + ProductMgr (TEMP profile)&quot;),
NOT($Profile.Name = &quot;MotoDev - Partnership Management - HOME&quot;)
)
)</errorConditionFormula>
        <errorMessage>This Device is FULLY APPROVED and cannot be edited.
Please contact Kirsten Rowe if changes need to be made.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disallow_editing_Internal_Name</fullName>
        <active>true</active>
        <description>Prevent selected user profiles from changing the Internal name of a device record</description>
        <errorConditionFormula>AND (ISCHANGED(Name),  OR($Profile.Name = &quot;MotoDev - Google TA Process&quot;, $Profile.Name = &quot;MotoDev - Google TA Process Admin&quot;))</errorConditionFormula>
        <errorMessage>You are not authorized to change the Internal Name of a Device!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GTA_GMS_AND_CTS_SW_BOM_Approved_Check</fullName>
        <active>true</active>
        <description>Unless device is canceled don&apos;t allow GMS or CTS Checklist fields to be filled in if SW BOM has not been approved. If SW BOM needs to be set back to No and GMS or CTS is filled then contact Dan Quicker.</description>
        <errorConditionFormula>AND( NOT(or($Profile.Name = &quot;MotoDev - SysAdmin&quot;,$Profile.Name = &quot;MotoDev - SW BOM&quot;,$Profile.Name = &quot;MotoDev - MotoReady Admin&quot;)), OR(AND(ISPICKVAL(SWBOM_Reviewed_and_Approved__c,&quot;No&quot;), NOT(ISBLANK(GMS_Software_Checklist__c))) , AND(ISPICKVAL(SWBOM_Reviewed_and_Approved__c,&quot;No&quot;), NOT(ISBLANK( CTS_Software_Checklist__c )))))</errorConditionFormula>
        <errorMessage>1.GMS or CTS Checklist can not be entered unless &quot;SW BOM Reviewed and Approved&quot; is set to &quot;Yes&quot;. 
2.SW BOM approval can not be set to &quot;No&quot; if GMS or CTS checklist is entered. 

To change to the SW BOM please contact dan.quicker@motorola.com</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GTA_Submitter_Email_Rule</fullName>
        <active>true</active>
        <description>If a Device is being submitted for Google TA - The GTA submitter email is a required field in order to notify the appropriate person of status and approval</description>
        <errorConditionFormula>AND
(ISBLANK (GTA_Submitter_Email__c), ISPICKVAL( GMS_Submission_Status__c , &quot;Checklist Provided&quot;),NOT( $Profile.Name = &quot;MotoDev - SysAdmin&quot;))</errorConditionFormula>
        <errorDisplayField>SW_Integration_Lead_Email__c</errorDisplayField>
        <errorMessage>Email address is required if submitting for Google TA</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Need_all_AIF_Approvals_for_SWBOM_Review</fullName>
        <active>true</active>
        <description>Requires all apps to be AIF Approved or Conditional, prior to selecting the &quot;SWBOM Reviewed and Approved&quot; checkbox</description>
        <errorConditionFormula>and(text(SWBOM_Reviewed_and_Approved__c )=&quot;YES&quot; ,  
of_Apps_Approved_on_BOM__c  &lt;1,  
 AND($User.ProfileId  &lt;&gt; &apos;00e40000000oqQ9&apos;,$User.ProfileId  &lt;&gt; &apos;00e40000000oqzn&apos;)     )</errorConditionFormula>
        <errorDisplayField>SWBOM_Reviewed_and_Approved__c</errorDisplayField>
        <errorMessage>Not all apps on this device have been AIF-approved yet.
All apps on this device need to be AIF-approved or Conditional, prior to setting the SWBOM Reviewed and Approved to YES</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Requested_Date_for_Google_TA</fullName>
        <active>true</active>
        <description>Requested date for Google TA required</description>
        <errorConditionFormula>AND 

(ISBLANK(Requested_Date_for_Google_TA__c),

ISPICKVAL( GMS_Submission_Status__c , &quot;Checklist Provided&quot;))</errorConditionFormula>
        <errorDisplayField>Requested_Date_for_Google_TA__c</errorDisplayField>
        <errorMessage>Requested date for Google TA required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SWBOM_Approver_Field_Filled</fullName>
        <active>true</active>
        <description>Must have SWBOM Approver field filled out if SWBOM has been reviewed</description>
        <errorConditionFormula>AND(ISPICKVAL(SWBOM_Reviewed_and_Approved__c , &quot;Yes&quot;),
SW_BOM_List_Approver__c=&quot;&quot;)</errorConditionFormula>
        <errorDisplayField>SW_BOM_List_Approver__c</errorDisplayField>
        <errorMessage>If SWBOM has been reviewed and approved, must select who reviewed it</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SW_Build_Folder_URL_Required</fullName>
        <active>true</active>
        <description>Link to SW build Folder is required when submitting a device for Google TA</description>
        <errorConditionFormula>AND 

(ISBLANK(GMS_Software_Checklist__c), 

ISPICKVAL(GMS_Submission_Status__c, &quot;Checklist Provided&quot;))</errorConditionFormula>
        <errorDisplayField>GMS_Software_Checklist__c</errorDisplayField>
        <errorMessage>Link to SW build Folder is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>X3rd_Party_Approved</fullName>
        <active>true</active>
        <description>A device can only be fully approved if its 3rd party apps requiring 3rd party approval are approved</description>
        <errorConditionFormula>AND( ISPICKVAL(Device_Fully_Approved__c, &quot;Yes&quot;), AND( 
( X3rd_Party_Apps_Approved_by_3rd_Party__c != Apps_Requiring_3rd_Party_Approval__c), 
NOT( $Profile.Name = &quot;MotoDev - SysAdmin&quot;),NOT( $Profile.Name = &quot;MotoDev - API User&quot;)))</errorConditionFormula>
        <errorMessage>This Device cannot be FULLY APPROVED because it has 3rd Party Apps still requiring 3rd Party Approval.</errorMessage>
    </validationRules>
