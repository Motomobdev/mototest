        <fullName>Region_Sort_Order__c</fullName>
        <externalId>false</externalId>
        <formula>CASE(TEXT(Account__r.Region__c), 
&apos;North America&apos;, 
IF(OR(ISPICKVAL(Account__r.PublicCalendar__c, &apos;Retail Sales (NA)&apos;),
      ISPICKVAL(Account__r.PublicCalendar__c, &apos;Retail Sales (Non NA)&apos;)), 4, 1), 
&apos;Latin America&apos;, 2, 
&apos;Europe Middle East &amp; Africa&apos;, 3, 
&apos;APAC&apos;, 5, 
&apos;Global&apos;, 6, 99)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Region Sort Order</label>
        <precision>18</precision>
        <required>false</required>
