    <validationRules>
        <fullName>Relate_Account</fullName>
        <active>true</active>
        <errorConditionFormula>and(text(Payment_Type__c) = &quot;Rev Share&quot; ,isblank(Account__c))</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>If this is a Rev Share entry, please relate it to an ACCOUNT</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rev_share_amounts_must_be_negative</fullName>
        <active>true</active>
        <description>If payment is Rev Share type, Amount should be negative number.</description>
        <errorConditionFormula>AND( ISPICKVAL( Payment_Type__c , &apos;Rev Share&apos;) , (  Amount__c   &gt;=  0 ) )</errorConditionFormula>
        <errorDisplayField>Amount__c</errorDisplayField>
        <errorMessage>A rev share payment must have a negative amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>cancelled_phase_and_status_must_match</fullName>
        <active>true</active>
        <description>If Phase is Cancelled, Status must also be Cancelled</description>
        <errorConditionFormula>IF( OR( AND( ISPICKVAL( Payment_Status__c , &apos;CANCELLED&apos;) , !ISPICKVAL( Payment_Phase__c , &apos;0 - Cancelled&apos;) ) , AND( !ISPICKVAL( Payment_Status__c , &apos;CANCELLED&apos;) , ISPICKVAL( Payment_Phase__c , &apos;0 - Cancelled&apos;) ) ) , true, false)</errorConditionFormula>
        <errorMessage>If Phase is &quot;0 - Cancelled&quot;, Status must also show as &quot;CANCELLED&quot; also if status is &quot;CANCELLED&quot; then Phase must be &quot;0 - Cancelled&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>paid_phase_and_status_must_match</fullName>
        <active>true</active>
        <description>If Phase is Paid, Status must also be PAID</description>
        <errorConditionFormula>IF( OR( AND( ISPICKVAL( Payment_Status__c , &apos;PAID&apos;) , !ISPICKVAL( Payment_Phase__c , &apos;6 - Paid Phase&apos;) ) , AND( !ISPICKVAL( Payment_Status__c , &apos;PAID&apos;) , ISPICKVAL( Payment_Phase__c , &apos;6 - Paid Phase&apos;) ) ) , true, false)</errorConditionFormula>
        <errorMessage>If Phase is &quot;6 - Paid Phase&quot;, Status must also show as &quot;PAID&quot; or is Status is &quot;PAID&quot; then phase but also be &quot;6 - Paid Phase&quot;</errorMessage>
    </validationRules>
