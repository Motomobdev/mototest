    <validationRules>
        <fullName>Cannot_Edit_ESR_After_Conversion</fullName>
        <active>false</active>
        <description>ESRs cannot be edited after they have been converted to agreements. The contract status and the total cost can change.</description>
        <errorConditionFormula>AND(
  NOT( 
    OR (ISCHANGED( Total_Cost__c ), 
        ISCHANGED( Contract_Status__c ),
        ISCHANGED(  Total_Depreciation_Expense_Amount__c),
        ISCHANGED(   Total_Capital_Amount__c ),
        ISCHANGED(   Total_Expense_Amount__c ),
        ISCHANGED(   Total_Non_Labor_Expense_Amount__c )
       )
    ), 
  NOT(
    ISBLANK(PRIORVALUE(Agreement__c ))
    )
  )</errorConditionFormula>
        <errorMessage>An agreement has been generated from this ESR and you are NOT allowed to make changes to the ESR.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_eGVM</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Account__r.Name = &apos;No eGVM Needed&apos;,  ISPICKVAL(Contract_Status__c, &apos;Pre-Approved: Negotiation in Progress&apos;))</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>Account must be specified other than &quot;	
No eGVM Needed&quot;</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Convert_to_an_agreement</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Convert to an agreement</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>account = &quot;{!External_Spend_Request__c.Account__c}&quot;;
agreement = &quot;{!External_Spend_Request__c.Agreement__c}&quot;;
contractStatus = &quot;{!External_Spend_Request__c.Contract_Status__c}&quot;;
motSignature = &quot;{!External_Spend_Request__c.Mot_Signature__c}&quot;;

if (account == &apos;&apos;) {
  alert(&apos;Error: This External Spend Request must be associated with an account prior to covnersion.&apos;);
} else if (agreement != &apos;&apos;) {
  alert(&apos;Error: An agreement has already been generated for this External Spend Request.&apos;);
} else if (contractStatus != &apos;Pre-Approved: Negotiation in Progress&apos;) {
  alert(&apos;Error: Contract status must be &quot;Pre-Approved: Negotiation in Progress.&apos;);
} else if (motSignature == 0) {
  alert(&apos;Error: There is no need to create an agreement because this External Spend Request  does not require Motorola Signature.&apos;);
} else {
  window.parent.location.href = &quot;/apex/APTPS_ESR_Integration?id=&quot; + &quot;{!External_Spend_Request__c.Id}&quot;
}</url>
    </webLinks>
