    <fields>
        <fullName>Playbook_Name__c</fullName>
        <externalId>false</externalId>
        <formula>IF 
(ISBLANK(Description__c), 
Account__r.Name &amp; &quot; &quot; &amp; TEXT(Period_Year__c ) &amp; &quot; - &quot; &amp; TEXT(Period_Quarter__c), Account__r.Name &amp; &quot; &quot; &amp; TEXT(Period_Year__c ) &amp; &quot; - &quot; &amp; TEXT(Period_Quarter__c) &amp; &quot; - &quot; &amp; Description__c)</formula>
