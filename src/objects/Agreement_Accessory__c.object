    <validationRules>
        <fullName>Accessory_Price_Requires_Interested</fullName>
        <active>true</active>
        <description>The user has to check &quot;Interested?&quot; checkbox for entering the price</description>
        <errorConditionFormula>AND(NOT(ISNULL(Invoice_Price__c)), Interested__c = false)</errorConditionFormula>
        <errorDisplayField>Interested__c</errorDisplayField>
        <errorMessage>Check Interested?</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Interested_Requires_Price</fullName>
        <active>true</active>
        <description>The user has to enter price if Interested? is checked.</description>
        <errorConditionFormula>AND(ISNULL(Invoice_Price__c), Interested__c)</errorConditionFormula>
        <errorDisplayField>Invoice_Price__c</errorDisplayField>
        <errorMessage>Enter price</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MAX_Deal_Requires_2013_Mot_Accessories</fullName>
        <active>true</active>
        <description>The Motorola Accessory invoice price cannot be blank if the user opts in for an accessory.</description>
        <errorConditionFormula>AND( Product_Type__c = &apos;Accessory-MOTO&apos;, X2013_Selected__c,  ISNULL(X2013_Invoice_Price__c))</errorConditionFormula>
        <errorDisplayField>X2013_Invoice_Price__c</errorDisplayField>
        <errorMessage>Invoice price for 2013 is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MAX_Deal_Requires_2014_Mot_Accessories</fullName>
        <active>true</active>
        <description>The Motorola Accessory invoice price cannot be blank if the user opts in for an accessory.</description>
