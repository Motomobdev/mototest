    <validationRules>
        <fullName>Application_must_have_an_Account</fullName>
        <active>false</active>
        <description>App must have a related Account</description>
        <errorConditionFormula>(Account__c )=&quot;&quot;</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>There must be a related Account for this Application</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Carrier_Branded_required_if_Carrier_Ap</fullName>
        <active>true</active>
        <description>Carrier Branded field cannot be blank if an app is Type &quot;Carrier App&quot;</description>
        <errorConditionFormula>and(ISPICKVAL(Application_Type__c ,&quot;Carrier App&quot;) ,ISPICKVAL(Carrier_Branded__c ,&quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Carrier_Branded__c</errorDisplayField>
        <errorMessage>If this is a Carrier App, please select a value for the field &quot;Carrier Branded&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Carrier_Furnished_required_if_Carrier_Ap</fullName>
        <active>true</active>
        <description>Carrier Furnished field cannot be blank if an app is Type &quot;Carrier App&quot;</description>
        <errorConditionFormula>and(ISPICKVAL(Application_Type__c ,&quot;Carrier App&quot;) ,ISPICKVAL( Carrier_Furnished__c,&quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Carrier_Furnished__c</errorDisplayField>
        <errorMessage>If this is a Carrier App, please select a value for the field &quot;Carrier Furnished&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Carrier_Mandated_required_if_Carrier_Ap</fullName>
        <active>true</active>
        <description>Carrier Mandated field cannot be blank if an app is Type &quot;Carrier App&quot;</description>
        <errorConditionFormula>and(ISPICKVAL(Application_Type__c ,&quot;Carrier App&quot;) ,ISPICKVAL(  Carrier_Mandated__c ,&quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Carrier_Mandated__c</errorDisplayField>
        <errorMessage>If this is a Carrier App, please select a value for the field &quot;Carrier Mandated&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_for_Single_GOV</fullName>
        <active>true</active>
        <description>Checks to ensure only a single contract is GOV</description>
        <errorConditionFormula>Count_of_Gov__c &gt;1</errorConditionFormula>
        <errorMessage>Error:  Only one related Contract may be checked as &quot;Governing&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contracts_Current_Phase_Require_Royalty</fullName>
        <active>false</active>
        <description>If an app is related to a contract in Phase &quot;Approvals&quot; or &quot;Execution and Processing&quot; , ensure that the three Royalty fields on the app record (High, Low, Current) are populated as well as the field &quot;Royalty Payment Attribute&quot;.</description>
        <errorConditionFormula>AND(OR(Application_Contract_Phase__c == &apos;Approvals&apos;, Application_Contract_Phase__c == &apos;Execution and Processing&apos;), OR(  ISNULL( Royalty_Current__c ),  ISNULL( Royalty_High__c ) ,  ISNULL( Royalty_Low__c ), ISPICKVAL( Royalty_Payment_Attribute__c , &apos;&apos;)  ))</errorConditionFormula>
        <errorMessage>This application is tied to a contract in Phase &quot;Approvals&quot; or &quot;Execution and Processing&quot;. You must select an appropriate royalty payment attribute and enter values for Royalty High, Royalty Current and Royalty Low fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_required_if_Exclusive_1</fullName>
        <active>true</active>
        <description>Date required if the Exclusive box is checked</description>
        <errorConditionFormula>AND(Exclusivity__c  = TRUE,isnull( Exclusivity_Start__c))</errorConditionFormula>
        <errorDisplayField>Exclusivity_Start__c</errorDisplayField>
        <errorMessage>&quot;Exclusive&quot; box is checked, so please enter a START Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_required_if_Exclusive_2</fullName>
        <active>true</active>
        <description>Date required if the Exclusive box is checked</description>
        <errorConditionFormula>AND(Exclusivity__c = TRUE,isnull( Exclusivity_End__c))</errorConditionFormula>
        <errorDisplayField>Exclusivity_End__c</errorDisplayField>
        <errorMessage>&quot;Exclusive&quot; box is checked, so please enter an END Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mot_Proprietary_API</fullName>
        <active>true</active>
        <description>If this app contains a mot proprietary api, then require them to be listed</description>
        <errorConditionFormula>ISPICKVAL( Leveraging_Motorola_Proprietary_APIs__c , &quot;Yes&quot;) &amp;&amp;
( Motorola_Proprietary_APIs_supported_list__c == NULL)</errorConditionFormula>
        <errorMessage>If the Leveraging Motorola Proprietary API picklist is marked yes, then enter the motorola proprietary api supported list</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_executed_contract_without_royalty</fullName>
        <active>false</active>
        <errorConditionFormula>IF( AND( ISCHANGED( Application_Contract_Status__c ), Application_Contract_Status__c = &apos;Fully Executed&apos;, OR( ISNULL(Royalty_Low__c ) , ISNULL(Royalty_High__c ) ) ) , true, false)</errorConditionFormula>
        <errorMessage>Contract cannot be set to Fully Executed without it&apos;s Royalty High/Low/Current fields being filled out.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Preload_Apps_must_has_SubType</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL( Application_Type__c , &apos;Preload&apos;)  &amp;&amp;  ISPICKVAL( Application_Sub_Type__c ,  &apos;&apos; )</errorConditionFormula>
        <errorDisplayField>Application_Sub_Type__c</errorDisplayField>
        <errorMessage>Must select a SubType for Preload Applications.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Available_for_Use</fullName>
        <active>false</active>
        <description>Prior to &quot;Automate Partner Checklist Approval&quot; being checked, &quot;Available for Use&quot; should be checked.</description>
        <errorConditionFormula>AND( Automate_Partner_Checklist_Approval__c == true,  Available_For_Use__c == false )</errorConditionFormula>
        <errorMessage>&apos;Available for Use&apos; MUST be checked if &apos;Automate Partner Checklist Approval&apos; gets checked.</errorMessage>
    </validationRules>
