        <fullName>Week_Number__c</fullName>
        <externalId>false</externalId>
        <formula>Case(
MOD( DATE(YEAR(Snapshot_Date__c), 1, 1) - DATE (1900, 1, 7), 7),
0, ( Snapshot_Date__c- DATE(YEAR(Snapshot_Date__c),1,1) + 3) /7, 
1, ( Snapshot_Date__c- DATE(YEAR(Snapshot_Date__c),1,1) + 4) /7, 
2, ( Snapshot_Date__c- DATE(YEAR(Snapshot_Date__c),1,1) + 5) /7, 
3, ( Snapshot_Date__c- DATE(YEAR(Snapshot_Date__c),1,1) + 6) /7, 
4, ( Snapshot_Date__c- DATE(YEAR(Snapshot_Date__c),1,1) + 7) /7, 
5, ( Snapshot_Date__c- DATE(YEAR(Snapshot_Date__c),1,1) + 1) /7, 
6, ( Snapshot_Date__c- DATE(YEAR(Snapshot_Date__c),1,1) + 2) /7, 
99)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Week Number</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
