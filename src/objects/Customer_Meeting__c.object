    <webLinks>
        <fullName>Copy_Agenda_to_Weekly</fullName>
        <availability>online</availability>
        <description>Copy Agenda items to Weekly Report</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Copy Agenda to Weekly</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/29.0/connection.js&apos;)}

if({!ISNULL(Customer_Meeting__c.Agenda__c)}){
  alert(&apos;Meeting agenda is Empty, please update the agenda before submitting to Weekly&apos;);
} 
else {
  var weeklyItem = new sforce.SObject(&apos;Weekly_Report_Item__c&apos;);
  weeklyItem.Summary__c = &quot;{!JSENCODE(Customer_Meeting__c.Agenda__c)}&quot;;
  weeklyItem.Category__c =&apos;Highlights&apos;;
  weeklyItem.Function__c = &apos;GTM&apos;;
  weeklyItem.Current_Week__c = &apos;true&apos;;
  weeklyItem.Meeting__c = &apos;{!Customer_Meeting__c.Id}&apos;;
  weeklyItem.Include_In_Report__c = &apos;true&apos;;
  weeklyItem.Source_ID__c = &apos;{!Customer_Meeting__c.Id}&apos;;
  weeklyItem.Source_Link__c = &apos;https://&apos;+ window.location.hostname + &apos;/&apos; + &apos;{!Customer_Meeting__c.Id}&apos;;
  weeklyItem.Source_Type__c = &apos;Meeting&apos;;
  weeklyItem.Account__c = &apos;{!Customer_Meeting__c.AccountId__c}&apos;;
  weeklyItem.Playbook__c = &apos;{!Customer_Meeting__c.PlaybookId__c}&apos;;
  weeklyItem.Business__c = &apos;{!Customer_Meeting__c.Business__c}&apos;;

  result = sforce.connection.create([weeklyItem]);
  if(result[0].success == &apos;true&apos;)
  {
      alert(&apos;Weekly Report Item created from Agenda Successfully.&apos;);
      window.location.reload();
  }
}</url>
